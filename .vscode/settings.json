{
	"[html]": {
		"javascript.autoClosingTags": false,
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.associations": {
		"*.css": "css"
	},
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
